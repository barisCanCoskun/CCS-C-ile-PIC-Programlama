00A6:  NOP
....................    lcd_output_enable(1); 
00A7:  BSF    06.0
....................    delay_us(2); 
00A8:  GOTO   0A9
....................    lcd_output_enable(0); 
00A9:  BCF    06.0
00AA:  RETURN
